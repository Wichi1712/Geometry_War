        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Desplazamiento, dirección, disparo

/*controla la velocidad del player impidiendo que siga avanzando
cuando se deja de presionar direccion up*/
if (speed &gt;0){
    speed -=0.10;
}
else {speed =0;}

//Velocidad y direccion-------------
//Dirección hacia adelante a la velocidad de +0.5
if (keyboard_check( vk_up))
{
    //Se cambia image_angle por giro ya que el objeto no esta girando
    //solo su sprite, para evitar que se quede pegado con otro objeto
    direction = giro;//image_angle;
    speed += velocidad;
    instance_create( x, y, obj_propulsor)
}
//control de velocidad no mayor a 8
if (speed &gt;=velomax)
{
    //direction = image_angle;
    speed =velomax;
}
 
//direccion tecla abajo reduce la velocidad
if( keyboard_check( vk_down))
{ 
  //direction = image_angle;
  speed -= velocidad;
}

/*Giro derecha e izquierda*/
//Solo se modifica la variable giro
//Izquierda
if (keyboard_check( vk_left))
{
    //image_angle +=giro;
    giro +=4;
}
//Derecha
else if (keyboard_check( vk_right))
{
    //image_angle -=giro;
    giro -=4;
}



//Fin velocidad y direccion----------------


//deteccion de colision con objetos

if place_meeting(x + hspeed, y + vspeed, obj_parentpared)
{
    move_bounce_solid(self);
    speed =0;
}

if place_meeting(x + hspeed, y + vspeed, obj_paredmortal)
{
    move_bounce_solid(self);
    speed =1;
}

if place_meeting(x + hspeed, y + vspeed, obj_parentpared_fragil)
{
    move_bounce_solid(self);
    speed = 1;
}

if place_meeting(x + hspeed, y + vspeed, obj_parentParedblanda)
{
    move_bounce_solid(self);
    speed = 1;
}

if place_meeting(x + hspeed, y + vspeed, obj_parent_jefe)
{
    move_bounce_solid(self);
    speed = 0;
}

if place_meeting(x + hspeed, y + vspeed, obj_bodysnake)
{
    move_bounce_solid(self);
    speed = 0;
}

if place_meeting(x + hspeed, y + vspeed, obj_parent_movertical)
{
    move_bounce_solid(self);
    speed = 3.5;
}

if place_meeting(x + hspeed, y + vspeed, obj_parent_movhorizon)
{
    move_bounce_solid(self);
    speed = 3.5;
}

if place_meeting(x + hspeed, y + vspeed, obj_burbuja_jefemarino)
{
    move_bounce_solid(self);
    speed = 1;
}
//Fin colisiones

//Disparo
if ( keyboard_check ( ord("A")))
{
  if (instance_number( obj_balaT) &lt;8)
    instance_create( x, y, obj_balaT);  
}

//Disparo balaluz
if instance_exists( obj_jefe2)
{
   if ( keyboard_check_pressed ( ord( "L")))
      { if (instance_number( obj_balaluz) &lt;7)
             instance_create( x, y, obj_balaluz);
      }     
}

//Disparo ametralladora
if ( keyboard_check ( ord( "Z")))
{ if (instance_number( obj_ametralladora) &lt; 20)
    instance_create( x, y, obj_ametralladora);
}

//Disparo misil
if ( keyboard_check_pressed ( ord( "X")))
{ //if (instance_number( obj_balaT) &lt;7)
    instance_create( random_range( nave2.x, nave2.x + 3), random_range( nave2.y, nave2.y + 3), obj_misilmatriz);   
}

//Disparo laser
if ( keyboard_check ( ord( "S")))
{ if (instance_number( obj_balalaser) &lt; 100)
    instance_create( x, y, obj_balalaser);
}
